<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/coscann"
    app:layout_behavior="@string/appbar_scrolling_view_behavior"
    tools:context=".ListeFichiersCSV"
    tools:showIn="@layout/activity_liste_fichiers_csv">

    <ListView
        android:id="@+id/listViewFichiercsv"
        android:layout_width="@dimen/listwidth"
        android:layout_height="@dimen/listheight"
        android:background="@drawable/borderbouton"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.045" />

    <TextView
        android:id="@+id/textView9"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Cliquer pour ouvrir le fichier"
        android:textColor="@color/black"
        android:textSize="@dimen/tailletextboutonaccueil"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="@+id/listViewFichiercsv"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/listViewFichiercsv"
        app:layout_constraintTop_toBottomOf="@+id/listViewFichiercsv"
        app:layout_constraintVertical_bias="0.044" />

</androidx.constraintlayout.widget.ConstraintLayout>